#include <아이오스트림>

주 ()
{
    intn;
    int a[8192]={0};
    std::cin>>n;
    a[1]=-1;
    a[2]=-1;
    a[3]=1;
    a[4]=-1;
    a[5]=1;
    a[6]=2;
    a[7]=-1;
    a[8]=2;
    a[9]=3;
    a[10]=2;
    a[11]=3;
    a[12]=4;
    a[13]=3;
    
    for(int i=14;i<=n;++i){
        만약 (a[i-5]>=1){
            a[i]=a[i-5]+1;
        }else if(a[i-3]>=1){
            a[i]=a[i-3]+1;
        }else{
            a[i]=-1;
        }
    }
    std::cout<<a[n];
}
